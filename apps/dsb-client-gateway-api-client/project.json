{
  "root": "apps/dsb-client-gateway-api-client",
  "sourceRoot": "apps/dsb-client-gateway-api-client/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/dsb-client-gateway-api-client",
        "command": "tsc -b tsconfig.json"
      }
    },
    "generate-schema": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/dsb-client-gateway-api-client/src",
        "command": "ts-node -r ./generateSchema.ts"
      }
    },
    "generate-client": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/dsb-client-gateway-api-client",
        "command": "rm -rf ./src/client && orval && barrelsby -d ./src/client"
      }
    },
    "clean": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/dsb-client-gateway-api-client/src",
        "command": "rm -rf ./dist ./client ./schema.yaml"
      }
    },
    "build-all": {
      "builder": "@nrwl/workspace:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          "nx clean dsb-client-gateway-api-client",
          "nx generate-schema dsb-client-gateway-api-client",
          "nx generate-client dsb-client-gateway-api-client",
          "nx build dsb-client-gateway-api-client --skip-nx-cache"
        ]
      }
    }
  },
  "tags": []
}
